{
  "taskGroups": [
    {
      "taskSpec": {
        "runnables": [
          {
            "container": {
              "imageUri": "%s",
              "entrypoint": "python3",
              "commands": [
                "/main.py"
              ]
            },
            "environment": {
              "variables": {
                "data": "%s",
                "model_dir": "%s",
                "random_state": "%d"
              }
            }
          }
        ],
        "computeResource": {
          "cpuMilli": 2000,
          "memoryMib": 16000
        },
        "maxRetryCount": 1
      },
      "taskCount": "%d",
      "parallelism": "%d"
    }
  ],
  "allocationPolicy": {
    "instances": [
      {
        "policy": {
          "machineType": "e2-highmem-2"
        }
      }
    ]
  },
  "logsPolicy": {
    "destination": "CLOUD_LOGGING"
  }
}
